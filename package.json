{
  "name": "ng-cash",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "watch": "nodemon --ext ts --watch src --exec ts-node src",
    "start": "node -r ts-node/register src",
    "dev": "nodemon src/index.ts",
    "lint": "eslint . --fix --ext .js,.ts",
    "knex:migrate:make": "knex --knexfile ./src/db/knexfile.ts migrate:make",
    "knex:migrate:latest": "knex --knexfile ./src/db/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile ./src/db/knexfile.ts migrate:rollback"
  },
  "author": "Josu√© lobo <josuevitorp.lobo@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "joi": "^17.7.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  }
